{
  "compilerOptions": {
    "composite": true,
    "module": "esnext",
    "moduleResolution": "node"
  },
  "include": ["vite.config.ts"],
  "paths": {
    "@/*": ["/src/*"],
    "@data": ["/src/data"],
    "@models": ["/src/models"],
    "@components": ["/src/components"],
    "@views": ["/src/router/views"],
    "@router/": ["/src/router/*"],
    "@store": ["/src/store"],
    "@assets": ["/src/assets"],
    "@styles": ["/src/styles"],
    "@utils": ["/src/utils"],
  }
}
